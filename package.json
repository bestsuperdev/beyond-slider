{
  "name": "beyond-slider",
  "version": "1.0.0",
  "description": "a react component for slider",
  "main": "lib/index.js",
  "style": "lib/index.css",
  "scripts": {
    "start": "node ./server.js",
    "mock": "cd ./mock & json-server --watch db.json --routes routes.json --port 9001",
    "lint": "eslint ./src",
    "test": "karma start  --config karma.conf.js",
    "clear": "rimraf lib",
    "build": "npm run clear && cross-env NODE_ENV=production babel src --out-dir lib --plugins transform-es2015-modules-commonjs  && webpack --config webpack.style.config.js",
    "clear:example": "rimraf dist",
    "build:example": "npm run clear:example && cross-env NODE_ENV=production webpack --config webpack.config.js"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": ">=0.14.0",
    "react-dom": ">=0.14.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.3.13",
    "babel-plugin-transform-es3-property-literals": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.1.18",
    "babel-preset-react-hmre": "^1.1.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.0",
    "es3ify-webpack-plugin": "0.0.1",
    "eslint": "^1.10.2",
    "eslint-plugin-react": "^3.11.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.28.0",
    "json-server": "^0.8.4",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "opn": "^3.0.3",
    "postcss-clearfix": "^0.2.0",
    "postcss-loader": "^0.8.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bestsuperdev/beyond-slider.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bestsuperdev/beyond-slider/issues"
  },
  "homepage": "https://github.com/bestsuperdev/beyond-slider.git"
}
